{
  "$schema": "https://esm.sh/wrangler/config-schema.json",
  "name": "synapse-dev",
  "compatibility_date": "2025-09-01",
  "assets": {
    "directory": "./dist",
    "not_found_handling": "404-page"
  },
  "workers_dev": true,
  "preview_urls": true,

  "env": {
    "production": {
      "name": "synapse",
      "compatibility_date": "2025-09-01",
      "assets": {
        "directory": "./dist",
        "not_found_handling": "404-page"
      },
      "routes": [
        {
          "pattern": "synapse.filecoin.services",
          "custom_domain": true
        }
      ]
    }
  }
}
